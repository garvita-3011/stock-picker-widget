{
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "extends": "airbnb-base",
    "globals": {},
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "import"
      // "babel"
  ],
    "rules": {
      "no-console": 1,
      "class-methods-use-this": "off",
      "import/prefer-default-export": "off",
      "no-restricted-syntax": [
        "off"
      ],
      "no-param-reassign": ["error", {
        "props": true,
        "ignorePropertyModificationsFor": ["event", "request"]
      }],
      "no-underscore-dangle": "off",
      "no-plusplus": "off",
      "no-unused-vars": "error",
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error"
    }
  }
